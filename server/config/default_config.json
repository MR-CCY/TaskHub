{
  "server": {
    "host": "0.0.0.0",
    "port": 8082
  },
  "work": {
    "is_work": false,
    "master_host": "127.0.0.1",
    "master_port": 8082,
    "worker_host": "127.0.0.1",
    "worker_port": 8083,
    "heartbeat_interval_ms": 2000,
    "max_running_tasks": 1,
    "worker_id":"worker-1",
    "queues":["default"],
    "labels":["shell"]
  },
  "worker": {
    "select_strategy": "least-load"
  },
  "dispatch": {
    "max_retries": 2
  },
  "database": {
    "host": "0.0.0.0",
    "port": 8082,
    "db_path": "./taskhub.db",
    "migrations_dir": "./migrations"
  },
  "log": {
    "path": "./logs/taskhub.log",
    "maxRecords": 2000,
    "rotateBytes": 2048,
    "maxFiles": 5,
    "compress": false
  }
}
